{
  "version": 2,
  "framework": null,
  "rewrites": [
    { "source": "/confirmation", "destination": "/confirmation.html" },
    { "source": "/confirmation.html", "destination": "/confirmation.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "cleanUrls": true
} 