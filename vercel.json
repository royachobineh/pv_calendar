{
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/confirmation", "destination": "/confirmation.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "cleanUrls": true
} 